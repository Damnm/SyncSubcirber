{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RabbitMqConfiguration": {
    "HostName": "localhost",
    "Username": "rabbitmq",
    "Password": "Epay2023"
  },
  "SubscriberConfiguration": {
    /******* Common options *******/
    "CreateExchangeQueue": true,
    "ExchangeOrQueue": 1,
    "AutoAck": "false",
    /******* End common options *******/

    /******* Exchange options *******/
    "ExchangeOption": {
      "ExchangeName": "EPAY.ETC.Sync",
      "ExchangeType": "headers",
      "AlternateExchange": "EPAY.ETC.DLQ"
    },
    /******* End Exchange options *******/

    /******* Queue options *******/
    "QueueOptions": [
      {
        "QueueName": "LaneOutSync",
        "DeadLetterExchange": "EPAY.ETC.DLQ",
        "DeadLetterRoutingKey": "LaneOutSync.DLQ",
        "BindArguments": {
          "LaneId": "1",
          "MsgType": "Out",
          "x-match": "all"
        }
      },
      {
        "QueueName": "Fees", 
        "Durable": true,
        "AutoDelete": "false",
        "DeadLetterExchange": "EPAY.ETC.DLQ",
        "DeadLetterRoutingKey": "Fees.DLQ",
        "BindArguments": {
          "MsgType": "Fees",
          "LaneId": "1",
          "x-match": "all"
        }
      }
    ]
    /******* Queue options *******/
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=dbtest;User Id=postgres;Password=Abcd@12345;CommandTimeout=20;"
    //"DefaultConnection": "Server=10.10.74.40;Port=5432;Database=dbtest;User Id=postgres;Password=1;CommandTimeout=20;"
  }
}
